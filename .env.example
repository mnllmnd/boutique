# ================================
# Configuration Environnements
# ================================

# API Configuration
ENVIRONMENT=production
API_HOST=https://api.boutique.example.com/api

# Android Keystore
KEYSTORE_PATH=~/boutique-key.jks
KEYSTORE_PASSWORD=your_keystore_password
KEY_PASSWORD=your_key_password
KEY_ALIAS=boutique_key

# App Metadata
APP_NAME=Boutique
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1
APP_PACKAGE_NAME=com.mnllmnd.boutique

# Store Credentials
# Google Play
GOOGLE_PLAY_CONSOLE_EMAIL=your-email@gmail.com

# Apple (TestFlight/App Store)
APPLE_DEVELOPER_EMAIL=your-apple@icloud.com
APPLE_DEVELOPER_TEAM_ID=XXXXX
